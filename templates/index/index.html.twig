{% extends 'base.html.twig' %}

{% block title %}Hello IndexController!{% endblock %}

{% block body %}
<div class="timeline">
    {% for post in posts %}
        <div class="timeline-content">
            <div class="timeline-dot">
                <div class="circle"></div>
            </div>
            <twig:SmallBlogPost post="{{ post }}" />
        </div>
    {% endfor %}
</div>
<div class="misc">
    {% for post in posts %}
        {% if post.isPinned %}
            <twig:SmallBlogPost post="{{ post }}" hasJumpButton="true" />
        {% endif %}
    {% endfor %}
    <div class="friends">
        <h3>Friends</h3>
